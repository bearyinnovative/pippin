{
  "name": "pippin",
  "version": "1.0.0",
  "description": "Ooops...",
  "author": "hbc <me@hbc.rocks>",
  "private": true,
  "scripts": {
    "dev": "nodemon -w app.js -w nuxt.config.js --exec babel-node app.js",
    "build": "nuxt build && babel app.js --out-file app.prod.js",
    "start": "cross-env NODE_ENV=production node app.prod.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "engines": {
    "node": "7.6.0"
  },
  "babel": {
    "plugins": "transform-async-to-generator"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "bearychat": "1.0.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^4.1.0",
    "koa-json": "^2.0.2",
    "koa-router": "next",
    "leancloud-storage": "^2.1.2",
    "leanengine": "1.2.2",
    "nuxt": "latest",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
